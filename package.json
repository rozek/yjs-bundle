{
  "name": "yjs-bundle",
  "version": "0.1.6",
  "description": "bundles several yjs packages in a single module in order to avoid \"Yjs was already imported\" problems",
  "type": "module",
  "browser": "./dist/yjs-bundle.js",
  "module": "./dist/yjs-bundle.esm.js",
  "svelte": "./dist/yjs-bundle.esm.js",
  "types": "./dist/yjs-bundle.d.ts",
  "exports": {
    ".": {
      "require": "./dist/yjs-bundle.js",
      "import": "./dist/yjs-bundle.esm.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js && tsc && mv src/*.d.ts dist && rm -r dist/src && rm src/*.js*",
    "agadoo": "agadoo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozek/yjs-bundle.git"
  },
  "keywords": [
    "yjs",
    "y-websocket",
    "y-webrtc",
    "y-indexeddb",
    "y-keyvalue"
  ],
  "author": "Andreas Rozek <a.rozek@gmx.de> (https://www.rozek.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rozek/yjs-bundle/issues"
  },
  "homepage": "https://github.com/rozek/yjs-bundle#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "agadoo": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.7",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "y-indexeddb": "github:rozek/y-indexeddb",
    "y-lwwmap": "^0.1.9",
    "y-utility": "github:rozek/y-utility",
    "y-webrtc": "github:rozek/y-webrtc",
    "y-websocket": "github:rozek/y-websocket",
    "yjs": "github:rozek/yjs"
  }
}
